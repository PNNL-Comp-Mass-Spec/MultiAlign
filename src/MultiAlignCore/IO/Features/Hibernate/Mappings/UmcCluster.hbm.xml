<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="MultiAlignEngine" 
                   namespace="MultiAlignEngine.Features">

	<class name="clsCluster" table="T_Clusters" lazy="false">
		<id name="Id" column="Cluster_ID" type="int">
			<generator class="assigned" />
		</id>
		<property name="Mass" column="Mass" type="double" not-null="false" />
		<property name="MassCalibrated" column="Mass_Calibrated" type="double" not-null="false" />
		<property name="Net" column="NET" type="double" not-null="false" />
		<property name="NetAligned" column="NET_Aligned" type="double" not-null="false" />
		<property name="Scan" column="Scan_LC" type="int" not-null="false" />
		<property name="Charge" column="Charge" type="int" not-null="false" />
		<property name="DriftTime" column="Drift_Time" type="double" not-null="false" />
    <property name="MeanScore" column="Tightness_Score" type="double" not-null="false" />
    <property name="Ambiguity" column="Ambiguity_Score" type="double" not-null="false" />
    <property name="MemberCount" column="Member_Count" type="int" not-null="false" />
    <property name="DatasetMemberCount" column="Dataset_Member_Count" type="int" not-null="false" />
		<!--set name="UmcSet" inverse="true">
			<key column="ID"/>
			<one-to-many class="clsUMC"/>
		</set-->
		<!--set name="MassTagSet" inverse="true" table="CLUSTER_TO_MASS_TAG_MAP">
			<key column="CLUSTER_ID"/>
			<many-to-many column="MASS_TAG_ID" class="MassTag"/>
		</set-->
	</class>

</hibernate-mapping>