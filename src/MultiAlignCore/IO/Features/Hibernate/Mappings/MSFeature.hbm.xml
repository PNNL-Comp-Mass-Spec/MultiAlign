<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="PNNLOmics" 
                   namespace="PNNLOmics.Data.Features">

	<class name="MSFeatureLight" table="T_MSFeatures" lazy="false">		
    <composite-id>
      <key-property name="ID"                     column="FEATURE_ID" type="integer" />
      <key-property name="GroupID"                column="DATASET_ID" type="integer" />
    </composite-id>
    <property name="DriftTime"                    column="DriftTime"                  type="double"   not-null="false" />
    <property name="Score"                        column="FIT"                        type="double"   not-null="false" />
		<property name="Scan"                         column="SCAN_NUM"                   type="integer"  not-null="false" />
		<property name="ChargeState"                  column="CHARGE"                     type="int"      not-null="false" />
		<property name="Abundance"                    column="ABUNDANCE"                  type="long"     not-null="false" />
		<property name="Mz"                           column="MZ"                         type="double"   not-null="false" />
		<property name="MassMonoisotopicAverage"      column="AVERAGE_MW"                 type="double"   not-null="false" />
		<property name="MassMonoisotopic"             column="MONOISOTOPIC_MW"            type="double"   not-null="false" />
    <property name="MassMonoisotopicMostAbundant" column="MONOISOTOPIC_MW_ABUNDANT"   type="double"   not-null="false" />
	</class>
</hibernate-mapping>