<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="MultiAlignCore" 
                   namespace="MultiAlignCore.Data.SequenceData">

	<class name="DatabaseSearchSequence" table="T_DatabaseSearchResults" lazy="false">
    <composite-id>
      <key-property name="ID"                     column="Feature_Id" type="integer" />
      <key-property name="GroupID"                column="Dataset_Id" type="integer" />
    </composite-id>  
    <property name="Sequence"       column="Sequence"       type="string"   not-null="false" />
    <property name="Scan"           column="Scan"           type="integer"  not-null="false" />
    <property name="Reference"      column="Reference"      type="string"   not-null="false" />
    <property name="Score"          column="Score"          type="double"   not-null="false" />
    <property name="DeltaScore"     column="DeltaScore"     type="double"   not-null="false" />
    <property name="DeltaScore2"    column="DeltaScore2"    type="double"   not-null="false" />
    <property name="Rank"           column="Rank"           type="integer"  not-null="false" />
    <property name="Charge"         column="Charge"         type="integer"  not-null="false" />
    <property name="Mass"           column="Mass"           type="double"   not-null="false" />
    <property name="TrypticEnds"    column="TrypticEnds"    type="integer"  not-null="false" />                            
  </class>
</hibernate-mapping>  