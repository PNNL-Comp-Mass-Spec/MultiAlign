<Window x:Class="MultiAlign.Windows.Wizard.AnalysisOptionsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
            
         Icon="/MultiAlign;component/Resources/logo.png" 
             Title="Advanced Options"
            
             xmlns:macontrols="clr-namespace:MultiAlignParameterFileEditor;assembly=MultiAlignParameterFileEditor"
             d:DesignHeight="437" d:DesignWidth="562">
    <DockPanel        
        Margin="5">
        <Button 
            DockPanel.Dock="Bottom"
            IsCancel="True"
            Width="64"
            Height="32"
            Style="{DynamicResource ResourceKey=SimpleBrowseButtonStyle}" 
            HorizontalAlignment="Right"
            >
            Ok
        </Button>
        <ContentControl    
            Margin="5"
            Content="{Binding Editor}"/>
    </DockPanel>
</Window>
