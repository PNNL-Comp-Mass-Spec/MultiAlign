<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="MultiAlignEngine" 
                   namespace="MultiAlignEngine.Features">

	<class name="clsUMC" table="T_LCMS_Features" lazy="false">
		<composite-id>
			<key-property name="Id"                 column="Feature_ID" type="integer" />
			<key-property name="DatasetId"          column="Dataset_ID" type="integer" />
		</composite-id>		
		<property name="ClusterId"                column="Cluster_ID" type="int" not-null="false" index="idx_LCMSFeatures_Cluster" />
		<property name="ConformationId"           column="Conformation_ID" type="int" not-null="false" />
		<property name="Mass"                     column="Mass"           type="double" not-null="false" />
		<property name="MassCalibrated"           column="Mass_Calibrated" type="double" not-null="false" />
		<property name="Net"                      column="NET" type="double" not-null="false" />
		<property name="MZForCharge"              column="MZ" type="double" not-null="false" />
		<property name="Scan"                     column="Scan_LC" type="int" not-null="false" />
		<property name="ScanStart"                column="Scan_LC_Start" type="int" not-null="false" />
		<property name="ScanEnd"                  column="Scan_LC_End" type="int" not-null="false" />
		<property name="ScanAligned"              column="Scan_Aligned" type="int" not-null="false" />
		<property name="ChargeRepresentative"     column="Charge" type="short" not-null="false" />
		<property name="AbundanceMax"             column="Abundance_Max" type="double" not-null="false" />
		<property name="AbundanceSum"             column="Abundance_Sum" type="long" not-null="false" />
		<property name="DriftTime"                column="Drift_Time" type="double" not-null="false" />
		<property name="AverageInterferenceScore" column="Average_Interference_Score" type="double" not-null="false" />
		<property name="AverageDeconFitScore"     column="Average_Decon_Fit_Score" type="double" not-null="false" />		
	</class>

</hibernate-mapping>