<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="MultiAlignEngine" 
                   namespace="MultiAlignEngine.MassTags">

	<class name="clsMassTag" table="T_Mass_Tags" lazy="false">
		<id name="Id" column="Mass_Tag_ID" type="integer">
			<generator class="assigned" />
		</id>
		<property name="Mass" column="Mass" type="double" not-null="false" />
		<property name="NetAverage" column="NET_Average" type="double" not-null="false" />
		<property name="NetPredicted" column="NET_Predicted" type="double" not-null="false" />
		<property name="NetStandardDeviation" column="NET_Std_Dev" type="double" not-null="false" />
		<!--property name="XCorr" column="XCORR" type="double" not-null="false" /-->
		<property name="DiscriminantMax" column="Normalized_Score_Max" type="double" not-null="false" />
		<property name="MSGFSpecProbMax" column="MSGF_Spec_Prob_Max" type="double" not-null="false" />
		<!--property name="Charge1FScore" column="CHARGE_1_F_SCORE" type="float" not-null="false" />
		<property name="Charge2FScore" column="CHARGE_2_F_SCORE" type="float" not-null="false" />
		<property name="Charge3FScore" column="CHARGE_3_F_SCORE" type="float" not-null="false" /-->
		<property name="CleavageState" column="Cleavage_State" type="int" not-null="false" />
		<property name="Modifications" column="Modifications" type="string" not-null="false" />
		<property name="Peptide" column="Peptide" type="string" not-null="false" />
		<property name="PeptideEx" column="Peptide_Ex" type="string" not-null="false" />
		<property name="MSMSObserved" column="MS_MS_Observed" type="int" not-null="false" />
		<property name="ModCount" column="Mod_Count" type="int" not-null="false" />
		<!--set name="UmcClusterSet" table="CLUSTER_TO_MASS_TAG_MAP">
			<key column="MASS_TAG_ID"/>
			<many-to-many column="CLUSTER_ID" class="UmcCluster"/>
		</set>
		<set name="PeptideSet" inverse="true">
			<key column="ID"/>
			<one-to-many class="Peptide"/>
		</set>
		<set name="ModificationSet" inverse="true">
			<key column="ID"/>
			<one-to-many class="Modification"/>
		</set>
		<set name="ProteinSet" inverse="true"  table="MASS_TAG_TO_PROTEIN_MAP">
			<key column="MASS_TAG_ID"/>
			<many-to-many column="PROTEIN_ID" class="Protein"/>
		</set-->
	</class>

</hibernate-mapping>