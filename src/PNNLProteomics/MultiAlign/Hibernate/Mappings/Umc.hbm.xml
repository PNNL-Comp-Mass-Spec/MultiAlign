<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="PNNLProteomics" 
                   namespace="PNNLProteomics.MultiAlign.Hibernate.Domain">

	<class name="Umc" table="T_UMC">
		<id name="Id" column="ID" type="integer">
			<generator class="increment" />
		</id>
		<property name="DatasetId" column="DATASET_ID" type="int" not-null="false" />
		<many-to-one name="UmcCluster" class="UmcCluster" column="CLUSTER_ID" not-null="false" />
		<property name="Mass" column="MASS" type="double" not-null="false" />
		<property name="MassCalibrated" column="MASS_CALIBRATED" type="double" not-null="false" />
    <property name="MassStandardDeviation" column="MASS_STANDARD_DEVIATION" type="double" not-null="false" />
		<property name="Net" column="NET" type="double" not-null="false" />
		<property name="Scan" column="SCAN" type="short" not-null="false" />
		<property name="ScanStart" column="SCAN_START" type="short" not-null="false" />
		<property name="ScanEnd" column="SCAN_END" type="short" not-null="false" />
		<property name="ScanAligned" column="SCAN_ALIGNED" type="short" not-null="false" />
		<property name="ChargeRepresentative" column="CHARGE_REPRESENTATIVE" type="short" not-null="false" />
		<property name="ChargeMax" column="CHARGE_MAX" type="short" not-null="false" />
		<property name="SpectralCount" column="SPECTRAL_COUNT" type="short" not-null="false" />
		<property name="AbundanceMax" column="ABUNDANCE_MAX" type="double" not-null="false" />
		<property name="AbundanceSum" column="ABUNDANCE_SUM" type="double" not-null="false" />
	</class>

</hibernate-mapping>