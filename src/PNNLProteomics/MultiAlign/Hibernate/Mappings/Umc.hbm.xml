<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="MultiAlignEngine" 
                   namespace="MultiAlignEngine.Features">

	<class name="clsUMC" table="T_UMC" lazy="false">
		<id name="Id" column="ID" type="integer">
			<generator class="increment" />
		</id>
		<property name="DatasetId" column="DATASET_ID" type="int" not-null="false" />
		<property name="ClusterId" column="CLUSTER_ID" type="int" not-null="false" />
		<!--many-to-one name="UmcCluster" class="clsCluster" column="CLUSTER_ID" not-null="false" /-->
		<property name="Mass" column="MASS" type="double" not-null="false" />
		<property name="MassCalibrated" column="MASS_CALIBRATED" type="double" not-null="false" />
		<!--property name="MassStandardDeviation" column="MASS_STANDARD_DEVIATION" type="double" not-null="false" /-->
		<property name="Net" column="NET" type="double" not-null="false" />
		<property name="Scan" column="SCAN" type="int" not-null="false" />
		<property name="ScanStart" column="SCAN_START" type="int" not-null="false" />
		<property name="ScanEnd" column="SCAN_END" type="int" not-null="false" />
		<property name="ScanAligned" column="SCAN_ALIGNED" type="int" not-null="false" />
		<property name="ChargeRepresentative" column="CHARGE_REPRESENTATIVE" type="short" not-null="false" />
		<property name="ChargeMax" column="CHARGE_MAX" type="short" not-null="false" />
		<property name="SpectralCount" column="SPECTRAL_COUNT" type="short" not-null="false" />
		<property name="AbundanceMax" column="ABUNDANCE_MAX" type="double" not-null="false" />
		<property name="AbundanceSum" column="ABUNDANCE_SUM" type="double" not-null="false" />
		<property name="DriftTime" column="DRIFT_TIME" type="float" not-null="false" />
		<property name="MZForCharge" column="MZ" type="double" not-null="false" />
	</class>

</hibernate-mapping>