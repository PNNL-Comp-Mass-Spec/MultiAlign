<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="PNNLProteomics" 
                   namespace="PNNLProteomics.MultiAlign.Hibernate.Domain">

	<class name="StacFDR" table="T_STAC_FDR" lazy="false">
		<id name="StacCutoff" column="STAC_Cutoff" type="double">
			<generator class="increment" />
		</id>
		<property name="NumMatches" column="Num_Matches" type="int" not-null="false" />
		<property name="NumFalseMatches" column="Num_False_Matches" type="int" not-null="false" />
		<property name="FDR" column="FDR" type="double" not-null="false" />
	</class>

</hibernate-mapping>