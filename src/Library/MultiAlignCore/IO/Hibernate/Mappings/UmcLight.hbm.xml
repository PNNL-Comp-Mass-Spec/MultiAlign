<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
                   assembly="PNNLOmics" 
                   namespace="PNNLOmics.Data.Features">

	<class name="UMCLight" table="T_LCMS_Features" lazy="false">
